package com.bhyf.pojo.dto.perm.tree;

import org.codehaus.jackson.annotate.JsonProperty;

import java.util.List;

public class AdditionalParameters {
	
	private List<AceTreeNode> children;
	
	@JsonProperty( "item-selected")//是否有选中
	private boolean itemSelected;
	
	public AdditionalParameters() {
		// TODO Auto-generated constructor stub
	}
	
	
	public AdditionalParameters(boolean itemSelected) {
		super();
		this.itemSelected = itemSelected;
	}


	public List<AceTreeNode> getChildren() {
		return children;
	}

	public void setChildren(List<AceTreeNode> children) {
		this.children = children;
	}

	public boolean isItemSelected() {
		return itemSelected;
	}

	public void setItemSelected(boolean itemSelected) {
		this.itemSelected = itemSelected;
	}
	
	
}
