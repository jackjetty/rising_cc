public class CheckApiAccountCommand {
	
	/**
	 * 环境编码
	 */
	private String envirCode;
	
	/**
	 * 真实IP
	 */
	private String realityIp;

	/**
	 * 请求报文
	 */
	private String request;

	/**
	 * 签名结果
	 */
	private String sign;
	
	public String getEnvirCode() {
		return envirCode;
	}

	public void setEnvirCode(String envirCode) {
		this.envirCode = envirCode;
	}

	public String getRealityIp() {
		return realityIp;
	}

	public void setRealityIp(String realityIp) {
		this.realityIp = realityIp;
	}

	public String getRequest() {
		return request;
	}

	public void setRequest(String request) {
		this.request = request;
	}

	public String getSign() {
		return sign;
	}

	public void setSign(String sign) {
		this.sign = sign;
	}
	
	
}
